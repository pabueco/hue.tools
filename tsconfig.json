{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "paths": {
      "$lib": [
        "src/lib"
      ],
      "$lib/*": [
        "src/lib/*"
      ],
      "$src/*": [
        "src/*"
      ],
      "$components/*": [
        "src/components/*"
      ]
    }
  }
}